  </servlet>
  <servlet-mapping>
    <servlet-name>DeleteServlet</servlet-name>
    <url-pattern>/DeleteServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ClearCartServlet</servlet-name>
    <servlet-class>cn.yq.web.controller.ClearCartServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ClearCartServlet</servlet-name>
    <url-pattern>/ClearCartServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>UpdateCartServlet</servlet-name>
    <servlet-class>cn.yq.web.controller.UpdateCartServlet</servlet-class>
